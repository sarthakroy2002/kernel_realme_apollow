&oplus_devinfo {
	aboard-gpio0 = <&pio 58 0x00>;
	aboard-gpio1 = <&pio 157 0x00>;
	aboard-gpio2 = <&pio 18 0x00>;
	aboard-gpio3 = <&pio 80 0x00>;
	aboard-gpio4 = <&pio 22 0x00>;
	pinctrl-names = "default", "aboard_gpio0_sleep", "aboard_gpio0_active",
			"aboard_gpio1_sleep", "aboard_gpio1_active",
			"aboard_gpio2_active","aboard_gpio3_active","aboard_gpio4_active";
	pinctrl-0 = <&hw_pins_default>;
	pinctrl-1 = <&aboard_pin_gpio0_sleep>;
	pinctrl-2 = <&aboard_pin_gpio0_active>;
	pinctrl-3 = <&aboard_pin_gpio1_sleep>;
	pinctrl-4 = <&aboard_pin_gpio1_active>;
	pinctrl-5 = <&aboard_pin_gpio2_active>;
	pinctrl-6 = <&aboard_pin_gpio3_active>;
	pinctrl-7 = <&aboard_pin_gpio4_active>;
};
&pio {
	hw_pins_default:default@hw_sub_id {
	};

	aboard_pin_gpio0_sleep:sleep@id0 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO58__FUNC_GPIO58>;
			input;
			bias-pull-down;
			input-enable;
		};
	};

	aboard_pin_gpio0_active:active@id0 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO58__FUNC_GPIO58>;
			input;
			bias-pull-down;
			input-enable;
		};
	};

	aboard_pin_gpio1_sleep:sleep@id1 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO157__FUNC_GPIO157>;
			input;
			bias-pull-down;
			input-enable;
		};
	};

	aboard_pin_gpio1_active:active@id1 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO157__FUNC_GPIO157>;
			input;
			bias-pull-down;
			input-enable;
		};
	};
	aboard_pin_gpio2_active:active@id2 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO18__FUNC_GPIO18>;
			slew-rate = <0>;
			bias-pull-down;
			input-enable;
		};
	};
	aboard_pin_gpio3_active:active@id3 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO80__FUNC_GPIO80>;
			slew-rate = <0>;
			bias-pull-down;
			input-enable;
		};
	};
	aboard_pin_gpio4_active:active@id4 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO22__FUNC_GPIO22>;
			slew-rate = <0>;
			bias-pull-down;
			input-enable;
		};
	};
};
