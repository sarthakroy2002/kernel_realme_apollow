        status = "okay";
        pw_en_gpio = <&pio 176 0x00>;
        vddio_en = <&pio 151 0x00>;
        power_source_1 {
            power-mode = <2>;
            power-name = "pw_en_gpio";
            poweron-level = <1>;
            delay-time = <0>;
        };
        power_source_2 {
            power-mode = <2>;
            power-name = "vddio_en";
            poweron-level = <0>;
            delay-time = <0>;
        };
    };
};

&goodix_fp {
    goodix,gpio_irq = <&pio 10 0x00>;
    goodix,gpio_reset = <&pio 43 0x00>;
    goodix,pw_en = <&pio 176 0x00>;
    //goodix,vddio_en = <&pio 151 0x00>;
    goodix,gpio_cs = <&pio 86 0x00>;
    pinctrl-names = "default", "gf_spi_drive_6mA", "gf_cs_func", "gf_spi_drive_6mA_pulllow";
    pinctrl-0 = <&gpio_goodix_irq_default>, <&gpio_goodix_reset_default>;
    pinctrl-1 = <&gpio_goodix_clk_6mA>, <&gpio_goodix_mosi_6mA>, <&gpio_goodix_miso_6mA>, <&gpio_goodix_cs_6mA>;
    pinctrl-2 = <&gpio_goodix_cs_func>;
    pinctrl-3 = <&gpio_goodix_clk_6mA_pulllow>, <&gpio_goodix_mosi_6mA_pulllow>, <&gpio_goodix_miso_6mA_pulllow>, <&gpio_goodix_cs_6mA_pulllow>;
    status = "okay";
};

/*the id config to project dts.*/
&pio{
	gpio_id0_default: gpio_id0_default {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO19__FUNC_GPIO19>;
			bias-pull-up = <00>;
			input-enable;
		};
	};
};

&oplus_fp_common {
	compatible = "oplus,fp_common"; //Manufacture, and Model
	oplus,fp_gpio_num = <1>;
	oplus,fp_gpio_0 = <&pio 19 0x00>;
	pinctrl-names = "default";
	pinctrl-0 = <&gpio_id0_default>;
	goodix_id {
		oplus,fp-id = <1>; /* fp id mapping table */
		vendor-chip = <11>; /* please refer to the struct fp_vendor_t in oppo_fp_commmon.n */
		eng-menu = "-1,-1"; /* represents SNR, inclination test item in engineer menu, and 0/1 means off/on */
		chip-name = "G_OPTICAL_G3";  /* /proc/fp_id, will be displayed in engineer menu  */
	};
	egis_id {
		oplus,fp-id = <0>; /* fp id mapping table */
		vendor-chip = <15>; /* please refer to the struct fp_vendor_t in oppo_fp_commmon.n */
		eng-menu = "-1,-1"; /* represents SNR, inclination test item in engineer menu, and 0/1 means off/on */
		chip-name = "E_OPTICAL_ET713";  /* /proc/fp_id, will be displayed in engineer menu  */
	};
};

